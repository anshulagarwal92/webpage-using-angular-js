                            <button type="button" id="closeBtn">X</button>
                            <img id="myImage" src="" alt=""/>
                        </div>

                        <div class="lightbox-background">&nbsp;</div>
                    <% } else { %>
                        <% for(var i = 0; i < allImages.length ; i++) {%>
                            <li style="list-style-type:none">
                                <a href="#" class="lessthanfive">
                                    <img style="width: 75%; height:150px;float:left" src="/uploads/dashboard_images/<%= allImages[i].img_url %>" alt=""/>
                                </a>
                            </li>
                        <% } %>
                    <% } %>
                    
                    <form action="/dashboardupload" method = "POST" enctype="multipart/form-data" >
                        <div class = "buttons">
                            <input style="margin: 17% 0% 0%; float: left;" type="file" name="image" accept="image/*">
                            <input style="margin: 17% 0% 0%; float: left;" type = "submit" value = " Upload" name = "submit">
                        </div>
                    </form>
                </div>
